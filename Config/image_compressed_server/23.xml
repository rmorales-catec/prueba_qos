                                    </locator>
                                </metatrafficUnicastLocatorList>
                            </RemoteServer>
                        </discoveryServersList>
                    </discovery_config>
                </builtin>
            </rtps>
        </participant>
        <!-- default publisher profile -->
        <publisher profile_name="image_publisher_qos" is_default_profile="true">
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </publisher>

        <!-- default subscriber profile -->
        <subscriber profile_name="image_subscriber_qos" is_default_profile="true">
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
        </subscriber>

         <!-- publisher profile for topic image_compressed -->
        <publisher profile_name="/image_compressed">
            <historyMemoryPolicy>DYNAMIC</historyMemoryPolicy>
            <qos>
                <publishMode>
                    <kind>SYNCHRONOUS</kind>
                </publishMode>
                <reliability>
                    <kind>BEST_EFFORT</kind>
                </reliability>
                <durability>
                    <kind>VOLATILE</kind>
                </durability>
            </qos>
            <topic>
                <historyQos>
                    <kind>KEEP_ALL</kind>
                    <depth>1000</depth>
                </historyQos>
            </topic>

